[production]
;Magento nstallation Stuff
magento.adminEmail = "jan@rocketweb.com"
magento.storeUrl = "http://magetesting.dev"

;this addresses go to account email notifications
magento.ftphost = "ftp://magetesting.com"
magento.dbhost = "db.magetesting.com"

magento.standardUser.instances = 3

magento.instanceTimeExecution = 1

magento.instanceprefix = "INST_"
magento.userprefix = "mi_"
magento.usersalt = "a63hd93u" ;used for mysql user password creation
magento.systemHomeFolder = "/home" ;where should instance user folders land in

google.analyticsId = ""

user.activationEmail.subject = "Activation"
user.activationEmail.from.email = "support@magetesting.com"
user.activationEmail.from.desc = "Magento Testing Team"

user.resetPassword.subject = "Reset Password"
user.resetPassword.from.email = "support@magetesting.com"
user.resetPassword.from.desc = "Magento Testing Team"

cron.queueItemReady.subject = "Your Instance is Ready!"
cron.queueItemReady.from.email = "support@magetesting.com"
cron.queueItemReady.from.desc = "Mage Testing Team"

cron.systemAccountCreated.subject = "Mage Testing Account!"
cron.systemAccountCreated.from.email = "support@magetesting.com"
cron.systemAccountCreated.from.desc = "Mage Testing Team"

register.useCoupons = 1; // 0 - dont show coupon field, 1 - show coupon field

;SQL Updater
; remember that this must be absolute path
sqlUpdater.directoryPath = APPLICATION_PATH "/../scripts/db";

;Extensions
extension.directoryPath = APPLICATION_PATH "/../data/extensions";

;Braintree
braintree.environment = ;this is either sandbox or production
;Following id/keys can be found after logging in to your braintree account (eg. sandbox.braintreegateway.com)
braintree.merchantId = 
braintree.publicKey = 
braintree.privateKey = 

;Found and setup at https://sandbox.braintreegateway.com/ -> Recurring Billing -> Plans
braintree.standardPlanId = 
braintree.businessPlanId = 

[staging : production]

[testing : production]

[development : production]