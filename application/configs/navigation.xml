<?xml version="1.0" encoding="UTF-8"?>
<configdata>
    <nav>
        <label>Mage Testing</label>
        <module>default</module>
        <controller>index</controller>
        <action>index</action>
        <route>default</route>
        <reset_params>true</reset_params>
        <pages>
            <dashboard>
                <label>Dashboard</label>
                <module>default</module>
                <controller>user</controller>
                <action>dashboard</action>
                <route>default</route>
                <reset_params>true</reset_params>
                <resource>default_user</resource>
                <privilege>dashboard</privilege>
                <icon>icon-white icon-list-alt</icon>
            </dashboard>
            <manage>
                <label>Manage</label>
                <module>default</module>
                <controller>user</controller>
                <action>index</action>
                <route>default</route>
                <reset_params>true</reset_params>
                <resource>default_queue</resource>
                <privilege>dashboard</privilege>
                <icon>icon-white icon-cog</icon>
                <class>dropdown dropdown-mainmenu</class>
                <dropdown>1</dropdown>
                <pages>
                    <supportedExtensions>
                        <label>Extensions</label>
                        <module>default</module>
                        <controller>extension</controller>
                        <action>index</action>
                        <route>default</route>
                        <reset_params>true</reset_params>
                        <resource>default_extension</resource>
                        <privilege>index</privilege>
                        <icon>icon-white icon-th-list</icon>
                    </supportedExtensions>
                    <plans>
                        <label>Plans</label>
                        <module>default</module>
                        <controller>plan</controller>
                        <action>index</action>
                        <route>default</route>
                        <reset_params>true</reset_params>
                        <resource>default_plan</resource>
                        <privilege>index</privilege>
                        <icon>icon-white icon-pencil</icon>
                    </plans>
                    <payments>
                        <label>Payments</label>
                        <module>default</module>
                        <controller>payment</controller>
                        <action>index</action>
                        <route>default</route>
                        <reset_params>true</reset_params>
                        <resource>default_payment</resource>
                        <privilege>index</privilege>
                        <icon>icon-white icon-briefcase</icon>
                    </payments>
                    <coupons>
                        <label>Coupons</label>
                        <module>default</module>
                        <controller>coupon</controller>
                        <action>index</action>
                        <route>default</route>
                        <reset_params>true</reset_params>
                        <resource>default_coupon</resource>
                        <privilege>index</privilege>
                        <icon>icon-white icon-tag</icon>
                    </coupons>
                    <wholeQueue>
                        <label>Stores</label>
                        <module>default</module>
                        <controller>queue</controller>
                        <action>index</action>
                        <route>default</route>
                        <reset_params>true</reset_params>
                        <resource>default_queue</resource>
                        <privilege>index</privilege>
                        <icon>icon-white icon-time</icon>
                    </wholeQueue>
                    <userList>
                        <label>Users</label>
                        <module>default</module>
                        <controller>user</controller>
                        <action>list</action>
                        <route>default</route>
                        <reset_params>true</reset_params>
                        <resource>default_user</resource>
                        <privilege>list</privilege>
                        <icon>icon-white icon-user</icon>
                    </userList>
                </pages>
            </manage>
            <userAccount>
                <label>My Account</label>
                <module>default</module>
                <controller>my-account</controller>
                <action>index</action>
                <route>default</route>
                <reset_params>true</reset_params>
                <resource>default_my-account</resource>
                <privilege>index</privilege>
                <icon>icon-white icon-user</icon>
            </userAccount>
            <signUp>
                <label>Sign Up</label>
                <module>default</module>
                <controller>index</controller>
                <action>our-plans</action>
                <route>compare</route>
                <reset_params>true</reset_params>
                <resource>default_user</resource>
                <privilege>register</privilege>
                <icon>glyphicons_044_keys_white_half.png</icon>
            </signUp>
            <userLogin>
                <label>Log In</label>
                <module>default</module>
                <controller>user</controller>
                <action>login</action>
                <route>default</route>
                <reset_params>true</reset_params>
                <resource>default_user</resource>
                <privilege>login</privilege>
                <icon>icon-white icon-arrow-down</icon>
            </userLogin>
            <help>
                <label>Help</label>
                <id>help</id>
                <uri>/help</uri>
                <resource>default_help</resource>
                <privilege>index</privilege>
                <icon>icon-white icon-question-sign</icon>
                <pages>
                    <helpImportingStore>
                        <label>Importing store</label>
                        <route>help-category</route>
                        <params>
                            <category>importing-store</category>
                        </params>
                        <reset_params>true</reset_params>
                        <pages>
                            <helpImportingStore>
                                <label>Changes in imported store.</label>
                                <route>help-page</route>
                                <params>
                                    <category>importing-store</category>
                                    <page>changes-in-imported-store</page>
                                </params>
                                <reset_params>true</reset_params>
                            </helpImportingStore>
                        </pages>
                    </helpImportingStore>
                    <helpDeploymentRollbacks>
                        <label>Deployment, rollbacks</label>
                        <route>help-category</route>
                        <params>
                            <category>deployment-rollbacks</category>
                        </params>
                        <reset_params>true</reset_params>
                    </helpDeploymentRollbacks>
                </pages>
            </help>
            <userLogout>
                <label>Logout</label>
                <module>default</module>
                <controller>user</controller>
                <action>logout</action>
                <route>default</route>
                <reset_params>true</reset_params>
                <resource>default_user</resource>
                <privilege>logout</privilege>
                <icon>icon-white icon-off</icon>
            </userLogout>
        </pages>
    </nav>
</configdata>